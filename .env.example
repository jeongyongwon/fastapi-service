# 애플리케이션 설정
APP_NAME=fastapi-service
APP_VERSION=1.0.0
ENVIRONMENT=production
DEBUG=false

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=app_db
DB_USER=postgres
DB_PASSWORD=your_password_here

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379

# 캐시 설정
CACHE_TTL=60
CACHE_MAX_SIZE=1000

# 로깅 설정
LOG_LEVEL=INFO
LOG_FORMAT=json

# API 설정
API_RATE_LIMIT=100
API_TIMEOUT=30
